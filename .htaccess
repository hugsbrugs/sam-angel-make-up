RewriteEngine On

#Redirection http:// vers http://www pour éviter le duplicate content
# LOCAL
RewriteCond %{HTTP_HOST} ^www.sam-angel-make-up.test$
RewriteRule ^(.*)   http://sam-angel-make-up.test/$1  [QSA,L,R=301] 

# 401 : Authorization Required :
ErrorDocument 401 "Authorisation nécessaire pour accéder à cette page. Contactez l'administateur de ce site internet."
# 403 : accès refusé :
ErrorDocument 403 http://sam-angel-make-up.test/Acces-Refuse
# 404 : Not Found !
ErrorDocument 404 http://sam-angel-make-up.test/Page-Introuvable
# 500 : Internal Server Error
ErrorDocument 500 "Erreur interne du serveur, veuillez réessayer plus tard ..."


RewriteRule ^maquillage$ html/maquillage.html [L,QSA]
RewriteRule ^onglerie$ html/onglerie.html [L,QSA]
RewriteRule ^soins-du-visage$ html/soins.html [L,QSA]
RewriteRule ^epilation$ html/epilation.html [L,QSA]

RewriteRule ^Acces-Refuse$ html/error.html [L,QSA]
RewriteRule ^Page-Introuvable$ html/error.html [L,QSA]